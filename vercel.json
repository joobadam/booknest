{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist/booknest/browser"
      }
    }
  ],
  "routes": [
    { "src": "/assets/(.*)", "dest": "/assets/$1" },
    { "src": "/(.+\\.(?:js|css|ico))", "dest": "/$1" },
    { "src": "/(.*)", "dest": "/index.html" }
  ]
}